    <%= simple_form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.input :name %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_confirmation %>
      <%= f.input :location %>
      <%= f.input :description %>
      <%= f.button :submit %>
    <% end %>
